contract LocalTrust {
    address addrA;
    address addrB;
    address addrC;
    principal low;
    assume {
        addrA => this;
    }

    uint highValue;
    uint{any} lowValue;

    constructor(address a) {
        addrA = a;
    }

    @public
    void setHigh{this}(uint v) {
        highValue = v;
    }

    @public
    void foo() {
        setHigh(0);
    }

    @public
    void setLow{any}(uint v) {
        lowValue = v;
    }
}
